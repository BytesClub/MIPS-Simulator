{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        },
        "allowImportExportEverywhere": false
    },
    "env": {
        "node": true,
        "commonjs": true
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-console": 0
    },
    "extends": [
        "eslint:recommended",
    ]
}
